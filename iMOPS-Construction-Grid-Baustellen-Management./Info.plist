<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDocumentTypes</key>
	<array>
		<!-- SKP (SketchUp) -->
		<dict>
			<key>CFBundleTypeName</key>
			<string>SketchUp Document</string>
			<key>CFBundleTypeRole</key>
			<string>Viewer</string>
			<key>LSHandlerRank</key>
			<string>Owner</string>
			<key>LSItemContentTypes</key>
			<array>
				<string>com.sketchup.skp</string>
			</array>
		</dict>
		<!-- 3D / CAD Formate -->
		<dict>
			<key>CFBundleTypeName</key>
			<string>3D Model</string>
			<key>CFBundleTypeRole</key>
			<string>Viewer</string>
			<key>LSHandlerRank</key>
			<string>Alternate</string>
			<key>LSItemContentTypes</key>
			<array>
				<string>com.pixar.universal-scene-description-mobile</string>
				<string>public.geometry-definition-format</string>
				<string>org.khronos.collada.digital-asset-exchange</string>
				<string>public.standard-tesselated-geometry-format</string>
				<string>org.khronos.gltf</string>
				<string>org.khronos.glb</string>
				<string>com.autodesk.fbx</string>
				<string>public.3d-content</string>
			</array>
		</dict>
	</array>
	<key>UTExportedTypeDeclarations</key>
	<array>
		<!-- SKP: iMOPS definiert den Typ fuer iOS (kein anderer App tut das) -->
		<dict>
			<key>UTTypeIdentifier</key>
			<string>com.sketchup.skp</string>
			<key>UTTypeDescription</key>
			<string>SketchUp Document</string>
			<key>UTTypeConformsTo</key>
			<array>
				<string>public.data</string>
				<string>public.content</string>
			</array>
			<key>UTTypeTagSpecification</key>
			<dict>
				<key>public.filename-extension</key>
				<array>
					<string>skp</string>
				</array>
				<key>public.mime-type</key>
				<array>
					<string>application/vnd.sketchup.skp</string>
					<string>application/octet-stream</string>
				</array>
			</dict>
		</dict>
	</array>
	<key>UTImportedTypeDeclarations</key>
	<array>
		<!-- FBX -->
		<dict>
			<key>UTTypeIdentifier</key>
			<string>com.autodesk.fbx</string>
			<key>UTTypeDescription</key>
			<string>Autodesk FBX</string>
			<key>UTTypeConformsTo</key>
			<array>
				<string>public.3d-content</string>
			</array>
			<key>UTTypeTagSpecification</key>
			<dict>
				<key>public.filename-extension</key>
				<array>
					<string>fbx</string>
				</array>
			</dict>
		</dict>
		<!-- glTF -->
		<dict>
			<key>UTTypeIdentifier</key>
			<string>org.khronos.gltf</string>
			<key>UTTypeDescription</key>
			<string>GL Transmission Format</string>
			<key>UTTypeConformsTo</key>
			<array>
				<string>public.3d-content</string>
			</array>
			<key>UTTypeTagSpecification</key>
			<dict>
				<key>public.filename-extension</key>
				<array>
					<string>gltf</string>
				</array>
			</dict>
		</dict>
		<!-- GLB -->
		<dict>
			<key>UTTypeIdentifier</key>
			<string>org.khronos.glb</string>
			<key>UTTypeDescription</key>
			<string>GL Binary Format</string>
			<key>UTTypeConformsTo</key>
			<array>
				<string>public.3d-content</string>
			</array>
			<key>UTTypeTagSpecification</key>
			<dict>
				<key>public.filename-extension</key>
				<array>
					<string>glb</string>
				</array>
			</dict>
		</dict>
	</array>
	<key>LSSupportsOpeningDocumentsInPlace</key>
	<true/>
	<key>UIFileSharingEnabled</key>
	<true/>
</dict>
</plist>
